 "html": "<!--<table class=\"table table-bordered\">-->\n<!--    <tr>-->\n<!--        <td style=\"color:white;\"></td>-->\n<!--        <td>{{doc.loan_account}}</td>-->\n<!--        <td style=\"color:white;\"></td>-->\n<!--        <td>{{doc.name}}</td>-->\n<!--        <td style=\"color:white;\">house No.:</td>-->\n<!--        <td>{{doc.house_number}}</td>-->\n<!--    </tr>-->\n<!--    <tr>-->\n<!--        <td></td>-->\n<!--    </tr>-->\n<!--     <tr>-->\n<!--        <td style=\"color:white;\">Due No.:</td>-->\n<!--        <td>{% if doc.due_number %}{{doc.due_number}}{% endif %}</td>-->\n<!--         <td style=\"color:white;\">Date:</td>-->\n<!--         {% if doc.posting_date %}-->\n        \n<!--        <td>{{doc.posting_date}}</td>-->\n<!--        {% endif %}-->\n        \n<!--    </tr>-->\n   \n<!--</table>-->\n\n\n<div class=\"col-xs-12\">\n        <div class=\"row section-break\">\n            <div class=\"col-xs-4\">\n                <div style=\"margin-top:0;margin-bottom:0;\" class=\"row data-field\">\n                    <div class=\"col-xs-3\"><label></label></div>\n                    <div class=\"col-xs-9 value\" style=\"text-align:right;\">{{doc.loan_account}}</div>\n                </div>\n                <div style=\"margin-top:15px;margin-bottom:0;\" class=\"row data-field\">\n                    <div class=\"col-xs-3\"><label></label></div>\n                    <div class=\"col-xs-9 value\" style=\"text-align:center;\">{% if doc.due_number %}{{doc.due_number}}{% endif %}</div>\n                </div><br>\n                \n            </div>\n            <div class=\"col-xs-4\">\n                <div style=\"margin-top:0;margin-bottom:0;\" class=\"row data-field\">\n                    <div class=\"col-xs-3\"><label></label></div>\n                    <div class=\"col-xs-9 value\" style=\"text-align:right;\"></div>\n                </div>\n                <br>\n                <div style=\"margin-top:0;margin-bottom:0;\" class=\"row data-field\">\n                    <div class=\"col-xs-3\"><label></label></div>\n                    <div class=\"col-xs-9 value\" style=\"text-align: right;padding-left: 20px;\">{{frappe.utils.get_datetime(doc.posting_date).strftime('%d-%m-%Y')}}</div>\n                </div><br>\n                \n            </div>\n            <div class=\"col-xs-4\">\n                <div style=\"margin-top:0;margin-bottom:0;\" class=\"row data-field\">\n                    <div class=\"col-xs-3\"><label></label></div>\n                    <div class=\"col-xs-9 value\" style=\"text-align:center;\">{% if doc.house_number %}{{doc.house_number}}{% endif %}</div>\n                </div><br>\n                \n                \n            </div>\n\n        </div>\n    </div>\n<div class=\"col-xs-12\" style=\"\n    margin-top: -25px;\n    padding-left: 25px;\">\n    <div class=\"row section-break\" >\n        <div class=\"col-xs-2\"></div>\n        <div class=\"col-xs-9\" style=\"text-align:left;\">{{doc.party_name.upper()}}</div>\n    </div>\n    <div class=\"row section-break\" >\n        <div class=\"col-xs-2\"></div>\n        <div class=\"col-xs-9\" style=\"text-align:left;margin-top: 10px;\">{{frappe.db.get_value(\"Loan Account\",doc.loan_account,\"address\")}}</div>\n    </div>\n     \n</div>\n\n{% set record_info = dict() %}\n{% set _dummy = record_info.update( {\"month\": \"None\" })  %}\n\n{% for due in doc.dues %}\n  {% if due.due_number == doc.due_number %}\n    {% set _dummy = record_info.update( {\"month\": frappe.utils.get_datetime(due.due_date).strftime('%B')  })  %}\n  {% endif %}\n{% endfor %}\n\n{% if record_info['month'] == \"None\" %}\n{% set _dummy = record_info.update( {\"month\": frappe.utils.get_datetime(doc.posting_date).strftime('%B') })  %}\n{% endif %}\n\n\n<table class=\"table table-bordered\" style=\"margin-bottom: 0px !important;\">\n\n    <tr>\n       <td><br></td>\n    </tr>\n    <tr>\n        <td width = \"75%\" style=\"text-align:right;\"> {{  record_info['month'] + \" - Due Amount\"  }}</td>\n        <td width = \"25%\" style=\"text-align:center;\">{{doc.paid_amount}}</td>\n    </tr>\n    <tr>\n       <td width = \"75%\" style=\"text-align:right;\"> {{ doc.remarks  }}</td>\n       <td width = \"25%\" style=\"text-align:center;\"></td>\n    </tr>\n   \n    <tr>\n        <td width = \"75%\" style=\"text-align:right;\"> Total</td>\n        <td width = \"25%\" style=\"text-align:center;\">{{doc.paid_amount}}</td>\n    </tr>\n    \n</table>\n<div class=\"col-xs-12\" style=\"padding-left: 25px;\">\n    <div class=\"row section-break\" >\n        <div class=\"col-xs-2\"></div>\n        <div class=\"col-xs-9\" style=\"text-align:left;\">{{ frappe.utils.money_in_words(doc.paid_amount)}}</div>\n    </div>\n</div>\n",
